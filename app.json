{
  "expo": {
    "name": "Puantaj Takip",
    "slug": "PuantajApp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./logolar/mipmap-xxxhdpi/ic_launcher.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./logolar/mipmap-xxxhdpi/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.puantajtakip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logolar/mipmap-xxxhdpi/ic_launcher_adaptive_fore.png",
        "backgroundImage": "./logolar/mipmap-xxxhdpi/ic_launcher_adaptive_back.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.puantajtakip",
      "minSdkVersion": 21,
      "targetSdkVersion": 33
    },
    "web": {
      "favicon": "./logolar/mipmap-xxxhdpi/ic_launcher.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cc628e6d-ebb4-467a-99ae-8e1b01810df7"
      }
    }
  }
}
